{
  "name": "utah",
  "version": "1.0.0",
  "description": "Utah Beachstorming Framework",
  "scripts": {
    "start": "nodemon --exec npm run buildRun",
    "oldstart": "node dist/index.js",
    "webpackstart": "webpack --config webpack.dev.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build-typecheck": "npm run build:types && npm run build:js",
    "buildRun": "npm run build && node dist/index.js",
    "build": "yarn clean:build && babel src --ignore src/__tests__ --out-dir dist --extensions '.ts,.tsx' --source-maps inline",
    "clean:build": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "coverage": "jest --coverage",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest -o --watch",
    "typecheck": "tsc"
  },
  "author": "Ilan Volow",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "github:types/express",
    "@types/jest": "^24.0.16",
    "@types/morgan": "^1.7.36",
    "@types/node": "^10.5.1",
    "@types/webpack-env": "^1.14.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^24.8.0",
    "nodemon-webpack-plugin": "^3.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.10.0",
    "typescript": "^3.5.3",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@types/mongoose": "^5.5.12",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.3",
    "mongodb": "^3.3.0",
    "mongoose": "^5.6.9",
    "morgan": "^1.9.1",
    "react": "^16.8.6"
  }
}
